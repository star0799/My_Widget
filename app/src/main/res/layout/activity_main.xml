<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"

    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity

    ">

    <ImageButton
        android:src="@drawable/a"

        android:maxHeight="40dp"
        android:minWidth="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibRotation"
        android:layout_alignTop="@+id/ibShock"
        android:layout_alignRight="@+id/ibBluetooth"
        android:layout_alignEnd="@+id/ibBluetooth" />

    <ImageButton
android:src="@drawable/b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibBluetooth"
        android:layout_marginTop="58dp"
        android:layout_below="@+id/ibWifi"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:src="@drawable/g"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_alignTop="@+id/ibLED"
        android:layout_toLeftOf="@+id/imageView4"
        android:layout_toStartOf="@+id/imageView4"
        android:layout_marginRight="57dp"
        android:layout_marginEnd="57dp" />

    <ImageButton
        android:src="@drawable/l"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibLock"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
android:src="@drawable/f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_above="@+id/ibBluetooth"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:src="@drawable/h"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_marginRight="60dp"
        android:layout_marginEnd="60dp"
        android:layout_alignTop="@+id/imageView3"
        android:layout_toLeftOf="@+id/ibLock"
        android:layout_toStartOf="@+id/ibLock" />

    <ImageButton
        android:src="@drawable/la"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibLED"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="89dp"
        android:layout_above="@+id/ibWifi"
        android:layout_alignRight="@+id/ibWifi"
        android:layout_alignEnd="@+id/ibWifi" />

    <ImageButton
        android:src="@drawable/s"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibShock"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_alignStart="@+id/imageView3" />

    <ImageButton
        android:src="@drawable/w"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibWifi"
        android:layout_alignTop="@+id/ibShock"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4" />




</RelativeLayout>
